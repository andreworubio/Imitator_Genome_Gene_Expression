# map reads with STAR

    # reads B11

    ./STAR --runMode alignReads --genomeDir $HOME/ar_imitator_genome/STAR_index --readFilesIn $HOME/ar_imitator_genome/raw_data/B11/B11_FRRB202335955-1a_H373HDSXY_L2_1.fq.gz $HOME/ar_imitator_genome/raw_data/B11/B11_FRRB202335955-1a_H373HDSXY_L2_2.fq.gz --readFilesCommand zcat --runThreadN 16 --outFilterMultimapNmax 20 --outFilterMismatchNmax 10
    --quantMode GeneCounts --outFileNamePrefix $HOME/ar_imitator_genome/STAR_mapped/B11 --outSAMtype BAM Unsorted SortedByCoordinate

